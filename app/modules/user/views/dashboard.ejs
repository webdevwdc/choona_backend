<% layout(layout_directory+'/mainLayout') -%>
    <!-- begin:: Content Head -->
    <div class="kt-subheader   kt-grid__item" id="kt_subheader">
        <div class="kt-subheader__main">
            <h3 class="kt-subheader__title">
                <%= page_title %>
            </h3>
        </div>
    </div>
    <!-- end:: Content Head -->

    <!-- begin:: Content -->
    <div class="kt-content  kt-grid__item kt-grid__item--fluid" id="kt_content">
                        
	<div class="row">
        <div class="col-lg-6">
            <div class="kt-portlet">
                <div class="kt-portlet__head">
                    <div class="kt-portlet__head-label">
                        <h3 class="kt-portlet__head-title">
                            User Summary
                        </h3>
                    </div>
                </div>
                <div class="kt-portlet__body">
                    <!-- new structure -->
                    <div class="kt-widget12">
                        <div class="kt-widget12__content">
                            <div class="kt-widget12__item">
                                <div class="kt-widget12__info">
                                    <span class="kt-widget12__desc">Total Users</span>
                                    <span class="kt-widget12__value">
                                        <%= response.totalUserCount %>
                                    </span>
                                </div>
                                <div class="kt-widget12__info">
                                    <span class="kt-widget12__desc">Active Users</span>
                                    <span class="kt-widget12__value">
                                        <%= response.activeUserCount %>
                                    </span>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- new structure -->
                </div>
            </div>
        </div>
    </div>
    </div>
    <!-- end:: Content -->

    <script>
        var fileref = document.createElement('script')
        fileref.setAttribute("type", "text/javascript")
        fileref.setAttribute("src", "/assets/app/custom/general/dashboard.js");
        document.getElementsByTagName("body")[0].appendChild(fileref);
    </script>